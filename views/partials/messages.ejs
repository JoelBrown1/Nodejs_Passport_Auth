<% if(typeof errors != 'undefined') { %>
  <% errors.forEach( (err) => { %>
    <% /* '%=' required to render an output value */ %>
    <div class="alert alert-warning alert-dismissible fade show" role="alert">
      <strong>Holy guacamole!</strong> <%= err.msg %>
      <button type="button" class="close close_icon" data-bs-dismiss="alert" aria-label="Close">X</button>
    </div>
    <% }) %>
<% } %>